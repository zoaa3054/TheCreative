{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/express-api/(.*)",
        "dest": "/api/server.js"
      }
    ],
    "installCommand": "npm install",
    "buildCommand": "node api/server.js"
  }